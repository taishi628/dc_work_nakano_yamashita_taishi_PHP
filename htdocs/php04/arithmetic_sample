  <?php  
$q_1 = '1'; //文字列型 シングル
$q_2 = "1";　//文字列型　ダブル
$q_3 = 1;//数型(integer)
$q_4 = 'true';//文字列型
$q_5 = true;    // 論理型(boolean)
$q_6 = 'null'; //null型
$q_7 = '';  //文字列型
$q_8 = 1.34;　//浮動小数点型(float)
$q_9;


var_dump($q_1);
print "\n";
var_dump($q_2);
print "\n";
var_dump($q_3);
print "\n";
var_dump($q_4);
print "\n";
var_dump($q_5);
print "\n";
var_dump($q_6);
print "\n";
var_dump($q_7);
print "\n";
var_dump($q_8);
print "\n";
var_dump($q_9);
    
   ?> 
